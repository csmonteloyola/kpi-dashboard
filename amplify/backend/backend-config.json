      "service": "Lambda"
    },
    "fnCollectGithub": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "fnCollectGithubCommits": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "api": {
